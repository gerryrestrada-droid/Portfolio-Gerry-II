# Copy to .env and fill values
# SMTP settings for nodemailer
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-smtp-username@example.com
SMTP_PASS=your-smtp-password

# Destination email that will receive contact form messages
# You can use your work or personal email. Example:
# TO_EMAIL=gerryr.estrada@urios.edu.ph
TO_EMAIL=twodyestrada@gmail.com

# Optional: server port
PORT=4000
